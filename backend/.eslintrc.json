{
  "extends": ["airbnb", "prettier"],
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "eqeqeq": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": [1, { "props": false }],
    "no-underscore-dangle": "off"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
